  		LDFLAGS="$LDFLAGS -L/usr/local/lib";;
    netbsd*)	CPPFLAGS="$CPPFLAGS -I/usr/pkg/include"
! 		LDFLAGS="$LDFLAGS -L/usr/pkg/lib";;
  esac
  
  #----------------------------------------------------------------
