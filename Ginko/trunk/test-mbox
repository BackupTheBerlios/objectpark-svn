*** dev-apps/libobjc/GNUmakefile.orig	Thu May 22 17:26:40 2003
--- dev-apps/libobjc/GNUmakefile	Mon Jun 23 10:04:19 2003
***************
*** 48,53 ****
--- 48,56 ----
  
  # dce, decosf1, irix, mach, os2, posix, pthreads, single, solaris, vxworks
  THREADING = posix
+ ifeq ($(GNUSTEP_TARGET_OS),netbsdelf)
+ THREADING = pth
+ endif
  ifeq ($(GNUSTEP_TARGET_OS),mingw32)
  THREADING = win32
  endif

